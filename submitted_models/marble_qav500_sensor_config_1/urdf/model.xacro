      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <geometry>
        <mesh filename='package://marble_qav500_sensor_config_1/meshes/neo11_propeller_cw.dae' scale='1 1 1'/>
      </geometry>
    </visual>
  </link>
  <link name='${name}/rotor_2'>
    <inertial>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <mass value='0.005'/>
      <inertia ixx='9.75e-07' ixy='0' ixz='0' iyy='8.13545e-05' iyz='0' izz='8.22545e-05'/>
    </inertial>
    <collision name='rotor_2_collision'>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <geometry>
        <cylinder radius='0.1397' length='0.005'/>
      </geometry>
    </collision>
    <visual name='rotor_2_visual'>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <geometry>
        <mesh filename='package://marble_qav500_sensor_config_1/meshes/neo11_propeller_ccw.dae' scale='1 1 1'/>
      </geometry>
    </visual>
  </link>
  <link name='${name}/rotor_3'>
    <inertial>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <mass value='0.005'/>
      <inertia ixx='9.75e-07' ixy='0' ixz='0' iyy='8.13545e-05' iyz='0' izz='8.22545e-05'/>
    </inertial>
    <collision name='rotor_3_collision'>
