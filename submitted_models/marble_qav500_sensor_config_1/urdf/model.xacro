<?xml version='1.0' encoding='utf-8'?>
<robot name="qav500" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:arg name="name" default="qav500"/>
  <xacro:property name="name" value="$(arg name)"/>
  <link name='${name}/base_link'>
    <inertial>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <mass value='3'/>
      <inertia ixx='0.075' ixy='0' ixz='0' iyy='0.075' iyz='0' izz='0.148916'/>
    </inertial>
    <collision name='base_link_inertia_collision'>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <geometry>
        <box size='0.3 0.3 0.25'/>
      </geometry>
    </collision>
    <visual name='base_link_inertia_visual'>
      <origin xyz='0 0 -0.12' rpy='0 -0 0'/>
      <geometry>
        <mesh filename='package://marble_qav500_sensor_config_1/meshes/qav500.stl' scale='1 1 1'/>
      </geometry>
    </visual>
    <visual name='camera_visual'>
      <origin xyz='0.27 0 0.03' rpy='0 -0 0'/>
      <geometry>
        <box size='0.025 0.09 0.025'/>
      </geometry>
    </visual>
    <visual name='laser_visual'>
      <origin xyz='0 0 -0.05' rpy='0 -0 0'/>
      <geometry>
        <cylinder radius='0.03' length='0.1'/>
      </geometry>
    </visual>
    <visual name='tof_top_visual'>
      <origin xyz='0.24 0 0.055' rpy='0 -0 0'/>
      <geometry>
        <box size='0.017 0.068 0.00735'/>
      </geometry>
    </visual>
    <visual name='tof_bottom_visual'>
      <origin xyz='0.24 0 0.006' rpy='0 -0 0'/>
      <geometry>
        <box size='0.017 0.068 0.00735'/>
      </geometry>
    </visual>
  </link>
  <sensor name='imu_sensor' update_rate='250'>
    <parent link='${name}/base_link'/>
    <origin xyz='0 0 0' rpy='0 -0 0'/>
  </sensor>
  <sensor name='air_pressure' update_rate='20'>
    <parent link='${name}/base_link'/>
    <origin xyz='0 0 0' rpy='0 -0 0'/>
  </sensor>
  <sensor name='magnetometer' update_rate='20'>
    <parent link='${name}/base_link'/>
    <origin xyz='0 0 0' rpy='0 -0 0'/>
  </sensor>
  <sensor name='camera_front' update_rate='30'>
    <parent link='${name}/base_link'/>
    <origin xyz='0.27 0 0.03' rpy='0 -0 0'/>
    <camera>
      <image width='1280' height='720' format='R8G8B8' hfov='1.5184' near='0.01' far='300' />
    </camera>
  </sensor>
  <sensor name='front_laser' update_rate='15'>
    <parent link='${name}/base_link'/>
    <origin xyz='0 0 -0.05' rpy='0 -0 0'/>
  </sensor>
  <sensor name='tof_top' update_rate='30'>
    <parent link='${name}/base_link'/>
    <origin xyz='0.24 0 0.055' rpy='0 -1.57 0'/>
    <camera>
      <image width='224' height='171' format='R8G8B8' hfov='1.047' near='0.1' far='4' />
    </camera>
  </sensor>
  <sensor name='tof_bottom' update_rate='30'>
    <parent link='${name}/base_link'/>
    <origin xyz='0.24 0 0' rpy='0 1.57 0'/>
    <camera>
      <image width='224' height='171' format='R8G8B8' hfov='1.047' near='0.1' far='4' />
    </camera>
  </sensor>
  <link name='${name}/rotor_0'>
    <inertial>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <mass value='0.005'/>
      <inertia ixx='9.75e-07' ixy='0' ixz='0' iyy='8.13545e-05' iyz='0' izz='8.22545e-05'/>
    </inertial>
    <collision name='rotor_0_collision'>
      <origin xyz='0 0 0' rpy='0 -0 0'/>
      <geometry>
