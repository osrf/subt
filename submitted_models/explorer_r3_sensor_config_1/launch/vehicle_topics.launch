<?xml version="1.0"?>
<launch>
  <arg name="world_name"/>
  <arg name="name"/>
  <arg name="link_name" value="base_link"/>
  <arg name="sensor_prefix" value="/world/$(arg world_name)/model/$(arg name)/link/$(arg link_name)/sensor"/>
  <arg name="breadcrumbs" default="0"/>
  <group ns="$(arg name)">
    <!--Create multipe bridges so that it can run in parallel-->
    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_pose"
      args="/model/$(arg name)/pose@tf2_msgs/TFMessage[ignition.msgs.Pose_V">
      <remap from="/model/$(arg name)/pose" to="pose"/>
    </node>

    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_pose_static"
      args="/model/$(arg name)/pose_static@tf2_msgs/TFMessage[ignition.msgs.Pose_V">
      <remap from="/model/$(arg name)/pose_static" to="pose_static"/>
    </node>

    <node
      pkg="subt_ros"
      type="pose_tf_broadcaster"
      name="pose_tf_broadcaster"/>

    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_twist"
      args="/model/$(arg name)/cmd_vel_relay@geometry_msgs/Twist]ignition.msgs.Twist">
      <remap from="/model/$(arg name)/cmd_vel_relay" to="cmd_vel"/>
    </node>

    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_odom"
      args="/model/$(arg name)/odometry@nav_msgs/Odometry[ignition.msgs.Odometry">
      <remap from="/model/$(arg name)/odometry" to="odom"/>
    </node>

    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_battery_state"
      args="/model/$(arg name)/battery/linear_battery/state@sensor_msgs/BatteryState[ignition.msgs.BatteryState">
      <remap from="/model/$(arg name)/battery/linear_battery/state" to="battery_state"/>
    </node>

    <!--Front RGBD camera -->
    <group ns="front">
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_camera_info"
        args="$(arg sensor_prefix)/front_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/front_camera/camera_info" to="color/camera_info"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_depth_info"
        args="$(arg sensor_prefix)/front_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/front_camera/camera_info" to="depth/camera_info"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_camera"
        args="$(arg sensor_prefix)/front_camera/image">
        <remap from="$(arg sensor_prefix)/front_camera/image" to="color/image"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_rgbd"
        args="$(arg sensor_prefix)/front_camera/points@sensor_msgs/PointCloud2[ignition.msgs.PointCloudPacked">
        <remap from="$(arg sensor_prefix)/front_camera/points" to="points"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_depth"
        args="$(arg sensor_prefix)/front_camera/depth_image">
        <remap from="$(arg sensor_prefix)/front_camera/depth_image" to="depth/image"/>
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_image_front">
        <remap from="input/image" to="color/image" />
        <remap from="output/image" to="optical/color/image" />
        <remap from="input/camera_info" to="color/camera_info" />
        <remap from="output/camera_info" to="optical/color/camera_info" />
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_depth_front">
        <remap from="input/image" to="depth/image" />
        <remap from="output/image" to="optical/depth/image" />
        <remap from="input/camera_info" to="depth/camera_info" />
        <remap from="output/camera_info" to="optical/depth/camera_info" />
      </node>
    </group>

    <!--Left RGBD camera -->
    <group ns="left">
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_camera_info"
        args="$(arg sensor_prefix)/left_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/left_camera/camera_info" to="color/camera_info"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_depth_info"
        args="$(arg sensor_prefix)/left_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/left_camera/camera_info" to="depth/camera_info"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_camera"
        args="$(arg sensor_prefix)/left_camera/image">
        <remap from="$(arg sensor_prefix)/left_camera/image" to="color/image"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_rgbd"
        args="$(arg sensor_prefix)/left_camera/points@sensor_msgs/PointCloud2[ignition.msgs.PointCloudPacked">
        <remap from="$(arg sensor_prefix)/left_camera/points" to="points"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_depth"
        args="$(arg sensor_prefix)/left_camera/depth_image">
        <remap from="$(arg sensor_prefix)/left_camera/depth_image" to="depth/image"/>
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_image_left">
        <remap from="input/image" to="color/image" />
        <remap from="output/image" to="optical/color/image" />
        <remap from="input/camera_info" to="color/camera_info" />
        <remap from="output/camera_info" to="optical/color/camera_info" />
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_depth_left">
        <remap from="input/image" to="depth/image" />
        <remap from="output/image" to="optical/depth/image" />
        <remap from="input/camera_info" to="depth/camera_info" />
        <remap from="output/camera_info" to="optical/depth/camera_info" />
      </node>
    </group>

    <!--Rear RGBD camera -->
    <group ns="rear">
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_camera_info"
        args="$(arg sensor_prefix)/rear_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/rear_camera/camera_info" to="color/camera_info"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_depth_info"
        args="$(arg sensor_prefix)/rear_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/rear_camera/camera_info" to="depth/camera_info"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_camera"
        args="$(arg sensor_prefix)/rear_camera/image">
        <remap from="$(arg sensor_prefix)/rear_camera/image" to="color/image"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_rgbd"
        args="$(arg sensor_prefix)/rear_camera/points@sensor_msgs/PointCloud2[ignition.msgs.PointCloudPacked">
        <remap from="$(arg sensor_prefix)/rear_camera/points" to="points"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_depth"
        args="$(arg sensor_prefix)/rear_camera/depth_image">
        <remap from="$(arg sensor_prefix)/rear_camera/depth_image" to="depth/image"/>
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_image_rear">
        <remap from="input/image" to="color/image" />
        <remap from="output/image" to="optical/color/image" />
        <remap from="input/camera_info" to="color/camera_info" />
        <remap from="output/camera_info" to="optical/color/camera_info" />
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_depth_rear">
        <remap from="input/image" to="depth/image" />
        <remap from="output/image" to="optical/depth/image" />
        <remap from="input/camera_info" to="depth/camera_info" />
        <remap from="output/camera_info" to="optical/depth/camera_info" />
      </node>
    </group>

    <!--Right RGBD camera -->
    <group ns="right">
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_camera_info"
        args="$(arg sensor_prefix)/right_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/right_camera/camera_info" to="color/camera_info"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_depth_info"
        args="$(arg sensor_prefix)/right_camera/camera_info@sensor_msgs/CameraInfo[ignition.msgs.CameraInfo">
        <remap from="$(arg sensor_prefix)/right_camera/camera_info" to="depth/camera_info"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_camera"
        args="$(arg sensor_prefix)/right_camera/image">
        <remap from="$(arg sensor_prefix)/right_camera/image" to="color/image"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_rgbd"
        args="$(arg sensor_prefix)/right_camera/points@sensor_msgs/PointCloud2[ignition.msgs.PointCloudPacked">
        <remap from="$(arg sensor_prefix)/right_camera/points" to="points"/>
      </node>
      <node
        pkg="ros_ign_image"
        type="image_bridge"
        name="ros_ign_image_depth"
        args="$(arg sensor_prefix)/right_camera/depth_image">
        <remap from="$(arg sensor_prefix)/right_camera/depth_image" to="depth/image"/>
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_image_right">
        <remap from="input/image" to="color/image" />
        <remap from="output/image" to="optical/color/image" />
        <remap from="input/camera_info" to="color/camera_info" />
        <remap from="output/camera_info" to="optical/color/camera_info" />
      </node>
      <node
        pkg="subt_ros"
        type="optical_frame_publisher"
        name="optical_frame_publisher_rgbd_depth_right">
        <remap from="input/image" to="depth/image" />
        <remap from="output/image" to="optical/depth/image" />
        <remap from="input/camera_info" to="depth/camera_info" />
        <remap from="output/camera_info" to="optical/depth/camera_info" />
      </node>
    </group>

    <!--Front Laser-->
    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_lidar3d"
      args="$(arg sensor_prefix)/front_laser/scan/points@sensor_msgs/PointCloud2[ignition.msgs.PointCloudPacked">
      <remap from="$(arg sensor_prefix)/front_laser/scan/points" to="points"/>
    </node>

    <!--IMU-->
    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_imu"
      args="$(arg sensor_prefix)/imu_sensor/imu@sensor_msgs/Imu[ignition.msgs.IMU">
      <remap from="$(arg sensor_prefix)/imu_sensor/imu" to="imu/data"/>
    </node>

    <!-- Gas sensor -->
    <node
      pkg="ros_ign_bridge"
      type="parameter_bridge"
      name="ros_ign_bridge_gas"
      args="/model/$(arg name)/gas_detected@std_msgs/Bool[ignition.msgs.Boolean">
      <remap from="/model/$(arg name)/gas_detected" to="gas_detected"/>
    </node>

    <group if="$(arg breadcrumbs)">
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_breadcrumbs"
        args="/model/$(arg name)/breadcrumb/deploy@std_msgs/Empty]ignition.msgs.Empty">
        <remap from="/model/$(arg name)/breadcrumb/deploy" to="breadcrumb/deploy"/>
      </node>
      <node
        pkg="ros_ign_bridge"
        type="parameter_bridge"
        name="ros_ign_bridge_breadcrumbs_remaining"
        args="/model/$(arg name)/breadcrumb/deploy/remaining@std_msgs/Int32[ignition.msgs.Int32">
        <remap from="/model/$(arg name)/breadcrumb/deploy/remaining" to="breadcrumb/remaining"/>
      </node>
    </group>
    
    <!-- Mapping Server -->
    <include file="$(find subt_ros)/launch/models_common/mapping_server_relays.launch">
      <arg name="name" value="$(arg name)"/>
    </include>
  </group>
</launch>

