            <sensor name="front_laser" type="gpu_ray">
				<!-- The laser is patterned off of the X1 medium range lidar.  
					We believe this is based on a Velodyne Puck Lite 
					which weighs 590 g -->
                <pose>0 0 0.05 0 0 0</pose>
                <update_rate>10</update_rate>
                <lidar>
                    <scan>
                        <horizontal>
